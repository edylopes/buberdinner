{
    "editor.fontFamily": "Fira Code, monospace",
    "editor.fontLigatures": true,
    "files.exclude": {
        "**/.git": true,
        "**/.gitignore": true,
        "**/.github": false,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/bin": true,
        "**/obj": true,
        "**/.config": true,
        "**/.idea": true,
        "**/assets": false
    },
    "files.watcherExclude": {
        "**/bin/**": true,
        "**/obj/**": true
    },
    "csharp.suppressDotnetRestoreNotification": true,
    "csharp.format.enable": true,
    "dotnet.codeLens.enableReferencesCodeLens": true,
    "dotnet.defaultSolution": "BuberDinner.sln",
    "docwriter.language": "English",
    "docwriter.progress.trackMethods": false,
    "docwriter.progress.trackFunctions": false
}
